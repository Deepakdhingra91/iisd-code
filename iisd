<script >
function readURL(input) {
    if (input.files && input.files[0]) {
        var reader = new FileReader();

        reader.onload = function (e) {
        	$("#img-view").children().attr('src', e.target.result);
        }

        reader.readAsDataURL(input.files[0]);
    }
}

$("#imgInp").change(function(){
    readURL(this);
});



	
$(document).ready(function(){
	$( "#file2" ).change(function() {
		 readURL(this);
		});

	$("#itiExaminationStream,#diplomaExaminationStream").change(function(e){
		if($($(this)[0]).val().trim()=="other"){
			$($(this)[0]).parent().children("input").show();
		}else{
			$($(this)[0]).parent().children("input").val("").hide();
		}
	});
	$("#domicile").change(function(e){
		if($($(this)[0]).val().trim()=="No"){
			$($(this)[0]).parent().children("input").show();
		}else{
			$($(this)[0]).parent().children("input").val("").hide();
		}
	});
	
	
	
		
});

function formValidation(){
	var b=true;
	/* if($("#file2").val().length<1){
